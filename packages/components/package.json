{
  "name": "@hashtag-design-system/components",
  "version": "0.1.8",
  "private": false,
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --setupFiles ./jest.config.ts",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "test:coverage": "react-scripts test --setupFiles ./jest.config.ts --coverage",
    "test:lint": "eslint . --ext .tsx, .ts --ignore-pattern node_modules/",
    "test:prettier": "prettier --config .prettierrc.json --config-precedence cli-override --write .",
    "css:var": "node src/utils/css/variablesToCss.js",
    "storybook": "start-storybook -p 3030 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublish": "yarn tsc && cp src/index.scss dist/ && cp -r src/styles dist/styles"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/hashtag-design-system/design_system",
  "author": "George Krachtopoulos <@georgekrax>",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hashtag-design-system/primitives": "^1.0.22",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@the_hashtag/common": "^1.0.29",
    "dayjs": "^1.10.3",
    "framer": "^1.2.12",
    "framer-motion": "^2.9.4",
    "libphonenumber-js": "^1.9.7",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-device-detect": "^1.15.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.14.0",
    "react-scripts": "4.0.1",
    "react-window": "^1.8.6",
    "sass": "^1.29.0",
    "swiper": "^6.4.9",
    "typescript": "^4.1.3",
    "ua-parser-js": "^0.7.23",
    "vanilla-tilt": "^1.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/node-logger": "^6.1.10",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.10",
    "@types/glob-base": "^0.3.0",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.167",
    "@types/react-test-renderer": "^17.0.0",
    "@types/react-window": "^1.8.2",
    "@types/swiper": "^5.4.1",
    "@types/ua-parser-js": "^0.7.35",
    "@types/vanilla-tilt": "^1.6.2",
    "css-loader": "^5.0.1",
    "jest": "^26.6.3",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "^10.1.0",
    "storybook-addon-designs": "^5.4.2",
    "style-loader": "^2.0.0"
  },
  "peerDependencies": {
    "@hashtag-design-system/primitives": "^1.0.22",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
