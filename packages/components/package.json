{
  "name": "@hashtag-design-system/components",
  "version": "0.1.88",
  "private": false,
  "scripts": {
    "dev": "yarn fresh-index && react-scripts start",
    "build": "rollup -c rollup.config.js",
    "prepack": "yarn gulp && yarn build",
    "test": "yarn index-tsx && react-scripts test --setupFiles ./jest.config.ts",
    "test:no-watch": "yarn test --watchAll=false",
    "test:coverage": "yarn test --coverage",
    "prettier": "prettier --config .prettierrc.json --config-precedence cli-override --write .",
    "storybook": "start-storybook -p 3030 -s public",
    "type-check": "yarn type-check:esm && yarn type-check:cjs",
    "type-check:esm": "gulp buildEsm",
    "type-check:cjs": "gulp buildCjs",
    "type-check:watch": "tsc -w",
    "gulp": "gulp",
    "concatScss": "gulp concatScss",
    "fresh": "gulp fresh",
    "copy-files": "gulp copyFiles",
    "index-tsx": "yarn fresh-index && yarn comment-index",
    "fresh-index": "gulp freshIndex",
    "comment-index": "gulp commentIndex",
    "build-storybook": "build-storybook -s public"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "repository": "https://github.com/hashtag-design-system/design_system",
  "author": "George Krachtopoulos <@georgekrax>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "types": "./build/index.d.ts",
  "files": [
    "/build"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hashtag-design-system/primitives": "1.2.16",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@the_hashtag/common": "^1.1.2",
    "dayjs": "^1.10.6",
    "framer-motion": "^4.1.17",
    "libphonenumber-js": "^1.9.23",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "react-scripts": "^4.0.3",
    "react-window": "^1.8.6",
    "swiper": "^6.8.1",
    "typescript": "^4.3.5",
    "ua-parser-js": "^0.7.28",
    "vanilla-tilt": "^1.7.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/node-logger": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.7",
    "@types/glob-base": "^0.3.0",
    "@types/jest": "^27.0.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.14",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-window": "^1.8.5",
    "@types/swiper": "^5.4.3",
    "@types/ua-parser-js": "^0.7.36",
    "@types/vanilla-tilt": "^1.6.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-replace": "^1.1.3",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.37.5",
    "sass-module-importer": "^1.4.0",
    "storybook-addon-designs": "^6.0.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@hashtag-design-system/primitives": ">=1.2.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
