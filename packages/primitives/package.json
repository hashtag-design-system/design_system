{
  "name": "@hashtag-design-system/primitives",
  "version": "1.2.12",
  "description": "The CSS (SCSS) primitives and brand guidelines of the #hashtag's design system",
  "main": "./src/index.scss",
  "scripts": {
    "fresh": "rm -rf node_modules && yarn",
